ORG 0000H
	
ADDR_A EQU 30H
ADDR_B EQU 40H
ADDR_C EQU 50H
	
LJMP START

ORG 0050H
	START:
	MOV ADDR_A, #03
	MOV ADDR_B, #04
	
	MOV A, ADDR_A
	ACALL SQR
	MOV R0, A
	
	MOV A, ADDR_B
	ACALL SQR
	
	ADD A, R0
	MOV ADDR_C, A
	
	SQR:
	MOV DPTR, #TABLE
	MOVC A, @A+DPTR
	RET
	
	TABLE:
	DB 0, 1, 4, 9, 16, 25, 36, 49
END
	